<div class='user-profile-div'>

  <ul>
    <% @users.each do |user| %>
      <li>  
        Name: <%= user.name %>
        <%= link_to 'See Profile',  user_path(user), class: 'profile-link' %>
        <span class="user-resquest">
          <%= friendship_interaction(user) %>
        </span>
      </li>
    <% end %>
  </ul>

</div>


